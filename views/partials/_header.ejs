<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button
      class="navbar-toggler"
      type="button"
      data-toggle="collapse"
      data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
      aria-expanded="false"
      aria-label="Toggle navigation"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
    </div>
    <% if (userEmail) { %>
      <h4 class="navbar-brand">Logged in as: <%= userEmail.email %></h4>
      <form class="form-inline" method="POST" action="/logout">
        <button type="submit" class="btn btn-outline-dark">Logout</button>
      </form>
    <% } else { %>
    <form class="form-inline" method="GET" action="/login">
      <div class="form-group mb-2">
        <button type="submit" class="btn btn-outline-dark">Login</button>
      </div>
    </form>
    &nbsp;
    <form class="form-inline" method="GET" action="/register">
      <div class="form-group mb-2">
        <button type="submit" class="btn btn-outline-dark">Register</button>
      </div>
    </form>
    <% } %>
  </nav>
</header>